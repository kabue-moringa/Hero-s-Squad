<div class="form">
  <form id="contact" action='/squad'>
    <h3>Add Your super Hero-Squad here</h3>
    <fields>
      <input id="squadname" name="squadname" placeholder="Squad Name" type="text" tabindex="1" required autofocus>
    </fields>
    <fields>
      <input id="membernumber" name="membernumber" placeholder="Number of Members" type="text" tabindex="3" required>
    </fields>
    <fields>
      <button style="width: 150px;" name="submit" type="submit" id="submit" data-submit="...Sending">Add Squad!</button>
    </fields>
  </form>
</div>
